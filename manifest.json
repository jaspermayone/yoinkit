{
  "manifest_version": 3,
  "name": "Media Extractor",
  "version": "1.0",
  "description": "Extract and download images, videos, and other media from webpages",
  "permissions": [
    "activeTab",
    "scripting"
  ],
  "action": {
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  },
  "background": {
    "service_worker": "background.js"
  }
}